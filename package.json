{
  "name": "pipocas-2021-frontend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./index.html --open",
    "start:homepage": "parcel ./homepage.html --open",
    "start:single-product": "parcel ./single-product.html --open",
    "start:product-list": "parcel ./product-list.html --open",
    "build:clean": "rm -r ./dist",
    "build": "npm run build:clean ; parcel build ./index.html",
    "build:gh-pages": "npm run build:clean ; parcel build ./index.html --public-url \"https://tresorama.github.io/pipocas-frontend/\"",
    "predeploy": "npm run build:gh-pages",
    "deploy": "gh-pages -d dist",
    "mustache-loghi": "mustache loghi-showcase-generator-list.json loghi-showcase-in.mustache > loghi-showcase.html"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "node-sass": "^6.0.1",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "sass": "^1.39.0"
  },
  "author": "Jacopo Marrone",
  "license": "ISC",
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "assets",
        "staticOutDir": "assets/"
      }
    ]
  },
  "dependencies": {
    "mustache": "^4.2.0"
  }
}
